{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.aux": true,
    "**/*.bbl": true,
    "**/*.bcf": true,
    "**/*.blg": true,
    "**/*.fdb_latexmk": true,
    "**/*.fls": true,
    "**/*.lof": true,
    "**/*.log": true,
    "**/*.lot": true,
    "**/*.out": true,
    "**/*.xml": true,
    "**/*.gz": true,
    "**/*.toc": true
  },
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "James-Yu.latex-workshop",
  "cSpell.language": "en,de",
  "cSpell.ignoreWords": ["scrreprt"],
  "cSpell.ignoreRegExpList": ["%.+", ".+={"],
  "latex-workshop.synctex.afterBuild.enabled": true,
  "latex-workshop.bibtex-format.sortby": ["title"],
  "latex-workshop.chktex.enabled": true,
  "latex-workshop.intellisense.citation.label": "title",
  "latex-workshop.intellisense.package.enabled": true,
  "latex-workshop.intellisense.unimathsymbols.enabled": true,
  "latex-workshop.intellisense.update.aggressive.enabled": true,
  "latex-workshop.latex.build.clearLog.everyRecipeStep.enabled": false,
  "latex-workshop.latex.outDir": "%DIR%/out",
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-utilities.liveReformat.enabled": true,
  "latex-utilities.zotero.latexCommand": "cite"
}
